  },
  "devDependencies": {
    "builder-victory-component-dev": "^2.1.1",
    "chai": "^3.2.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "react": "0.14.x",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "0.14.x",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
