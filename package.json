{
  "name": "victory",
  "version": "0.6.1",
  "description": "Data viz for React",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/formidablelabs/victory.git"
  },
  "engineStrict" : true,
  "engines": {
    "npm": ">=3.0.0"
  },
  "author": "Formidable",
